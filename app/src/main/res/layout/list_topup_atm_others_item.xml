<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_height="match_parent"
    android:layout_width="match_parent">

    <RelativeLayout android:layout_width="match_parent"
        android:layout_height="match_parent">

            <TextView android:id="@+id/title_atm"
                android:text="@string/other_bank"
                android:textColor="@color/colorSecondaryDark"
                android:textSize="@dimen/text_size_medium_material"
                android:layout_width="245dp"
                android:paddingLeft="24dp"
                android:layout_height="wrap_content"/>

            <LinearLayout android:id="@+id/list_atm_other"
                android:layout_below="@id/title_atm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="16dp"
                >

                <LinearLayout android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:padding="6dp"
                    android:layout_height="wrap_content">
                    <RelativeLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="4dp"
                        >
                        <ImageView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/red_ball_icon"
                            />
                        <TextView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="1"
                            android:textColor="@color/grey_1000w"
                            android:textSize="@dimen/text_size_small_material"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true"/>
                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/text_size_small_material"
                        android:textColor="@color/colorSecondaryDark"
                        android:text="@string/listatm_topup_other_ins_1"
                        android:layout_gravity="center"
                        />
                </LinearLayout>

                <LinearLayout android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:padding="6dp"
                    android:layout_height="wrap_content">
                    <RelativeLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="4dp"
                        >
                        <ImageView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/red_ball_icon"
                            />
                        <TextView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="2"
                            android:textColor="@color/grey_1000w"
                            android:textSize="@dimen/text_size_small_material"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true"/>
                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/text_size_small_material"
                        android:textColor="@color/colorSecondaryDark"
                        android:text="@string/listatm_topup_other_ins_2"
                        android:layout_gravity="center"
                        />
                </LinearLayout>

                <LinearLayout android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:padding="6dp"
                    android:layout_height="wrap_content">
                    <RelativeLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="4dp"
                        >
                        <ImageView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/red_ball_icon"
                            />
                        <TextView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="3"
                            android:textColor="@color/grey_1000w"
                            android:textSize="@dimen/text_size_small_material"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true"/>
                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/text_size_small_material"
                        android:textColor="@color/colorSecondaryDark"
                        android:text="@string/listatm_topup_other_ins_3"
                        android:layout_gravity="center"
                        />
                </LinearLayout>

                <LinearLayout android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:padding="6dp"
                    android:layout_gravity="center"
                    android:background="@color/colorSecondaryWhite"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/text_size_small_material"
                        android:text="@string/exempli_gratia"
                        android:textStyle="italic"
                        android:layout_marginLeft="10dp"
                        android:textColor="@color/grey_1000b"
                        android:layout_gravity="center"
                        />
                    <TextView
                        android:id="@+id/pin_account"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/text_size_small_material"
                        android:textStyle="italic"
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="4dp"
                        android:textColor="@color/grey_1000b"
                        android:layout_gravity="center"
                        android:textIsSelectable="true"
                        />
                </LinearLayout>

                <LinearLayout android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:padding="6dp"
                    android:layout_height="wrap_content">
                    <RelativeLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="4dp"
                        >
                        <ImageView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/red_ball_icon"
                            />
                        <TextView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="4"
                            android:textColor="@color/grey_1000w"
                            android:textSize="@dimen/text_size_small_material"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true"/>
                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/text_size_small_material"
                        android:textColor="@color/colorSecondaryDark"
                        android:text="@string/listatm_topup_other_ins_4"
                        android:layout_gravity="center"
                        />
                </LinearLayout>

                <LinearLayout android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:padding="6dp"
                    android:layout_gravity="center"
                    android:background="@color/colorSecondaryWhite"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/text_size_small_material"
                        android:text="@string/exempli_gratia"
                        android:textStyle="italic"
                        android:layout_marginLeft="10dp"
                        android:textColor="@color/grey_1000b"
                        android:layout_gravity="center"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="50000"
                        android:textSize="@dimen/text_size_small_material"
                        android:textStyle="italic"
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="4dp"
                        android:textColor="@color/grey_1000b"
                        android:layout_gravity="center"
                        />
                </LinearLayout>

                <LinearLayout android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:padding="6dp"
                    android:layout_height="wrap_content">
                    <RelativeLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="4dp"
                        >
                        <ImageView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/red_ball_icon"
                            />
                        <TextView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="5"
                            android:textColor="@color/grey_1000w"
                            android:textSize="@dimen/text_size_small_material"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true"/>
                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/text_size_small_material"
                        android:textColor="@color/colorSecondaryDark"
                        android:text="@string/listatm_topup_other_ins_5"
                        android:layout_gravity="center"
                        />


                </LinearLayout>
                <TextView
                    android:id="@+id/fee_deskripsi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:textStyle="bold"
                    android:layout_marginTop="8dp"
                    android:text="asdfasdfasdfa"
                    />
            </LinearLayout>

            <ImageView android:id="@+id/img_atm_other"
                android:src="@drawable/atm_other"
                android:layout_toRightOf="@id/title_atm"
                android:gravity="right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="4dp"
        android:background="@color/grey_400"
        android:id="@+id/divider"
        android:visibility="visible">
    </View>
</LinearLayout>